{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
  },
  "extends": "eslint:recommended",
  "globals": {
    "jQuery": true,
    "lazyload_video_settings": true,
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "modules": true,
    }
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": ["warn"],
    "semi": 2,
  }
}
